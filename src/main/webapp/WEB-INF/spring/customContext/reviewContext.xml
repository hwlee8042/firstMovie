<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
 	<bean id="reviewController" class="first.project.movie.review.controller.ReviewController" p:reviewService-ref="reviewService"/>
	<bean id="reviewService" class="first.project.movie.review.service.ReviewServiceImpl" p:reviewDAO-ref="reviewDAO"/>
	<bean id="reviewDAO" class="first.project.movie.review.dao.ReviewDAOImpl" p:sqlSession-ref="sqlSessionTemplate"/>
	<bean id="reviewVO" class="first.project.movie.review.model.ReviewVO"/>
</beans>