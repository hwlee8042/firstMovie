<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="memberListController" class="first.project.movie.admin.controller.MemberListController" p:memberListService-ref="memberListService"/>
	<bean id="memberListService" class="first.project.movie.admin.service.MemberListServiceImpl" p:memberListDAO-ref="memberListDAO"/>
	<bean id="memberListDAO" class="first.project.movie.admin.dao.MemberListDAOImpl" p:sqlSession-ref="sqlSessionTemplate"/>
	<bean id="memberListVO" class="first.project.movie.admin.model.MemberListVO" />
</beans>
